
<form action="/input" method = "post">
    <h4 class="p-2">Input</h4>
    <div class="form-group p-1">
        <input class="form-control" type="text" name="cedula" value="" placeholder="Cedula">
    </div>
    <div class="form-group p-1">
        <input class="form-control" type="text" name="nombres" value="" placeholder="Nombres">
    </div>
    <div class="form-group p-1">
        <input class="form-control" type="text" name="apellidos" value="" placeholder="Apellidos">
    </div>
    <div class="form-group p-1">
        <input class="form-control" type="text" name="celular" value="" placeholder="Celular">
    </div>
    <div class="form-group p-1">
        <select name="genero">
            <option value="Masculino">Masculino</option>
            <option value="Femenino">Femenino</option>
            <option value="Otros">Otros</option>
        </select>
    </div>
    <div class="form-group p-1 text-end">
        <button class="btn btn-primary" type="submit">Enviar</button>
    </div>
</form>